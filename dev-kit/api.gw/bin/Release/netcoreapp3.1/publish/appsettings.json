{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft": "Warning",
      "Microsoft.Hosting.Lifetime": "Information"
    }
  },
  "AllowedHosts": "*",
  "ServiceConfig": {
    "ServiceDiscoveryAddress": "http://consul-server-get-start:8500",
    "ServiceAddress": "http://api-gw:80",
    "ServiceName": "api-gw",
    "ServiceID": "api-gw",
    "Tags": [

      "traefik.enable=true",
      "traefik.http.routers.api-gw.rule=Host(`localhost`) && PathPrefix(`/api-gw`)",
      "traefik.http.middlewares.api-gw-stripprefix.stripprefix.prefixes=/api-gw",
      "traefik.http.routers.api-gw.middlewares=api-gw-stripprefix@consulcatalog",
      "traefik.tags=exposed"
    ]
  }
}
